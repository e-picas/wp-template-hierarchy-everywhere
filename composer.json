{
  "name": "picas/wp-template-hierarchy-everywhere",
  "description": "A WordPress plugin that implements the 'template hierarchy' concept for any template",
  "type": "wordpress-plugin",
  "keywords": [ "wordpress", "template", "template hierarchy", "theme" ],
  "homepage": "https://gitlab.com/e-picas/wp-template-hierarchy-everywhere",
  "license": "GPL-3.0+",
  "support": {
    "issues": "http://gitlab.com/e-picas/wp-template-hierarchy-everywhere/issues",
    "source": "http://gitlab.com/e-picas/wp-template-hierarchy-everywhere/tree/master"
  },
  "authors": [{
    "name": "Pierre Cassat",
    "homepage": "http://picas.fr"
  }],
  "require": {
    "php": ">=5.3.3",
    "composer/installers": "1.0.*@dev"
  },
  "require-dev": {
    "fabpot/php-cs-fixer": "@stable"
  },
  "scripts": {
    "cs-fixer": "php ./vendor/bin/php-cs-fixer fix wp-template-hierarchy-everywhere.php --level=psr2 -v"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.*-dev"
    }
  }
}
